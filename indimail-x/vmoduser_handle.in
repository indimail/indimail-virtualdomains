#!/bin/sh
verbose=0
toggle=0
args=$*
while getopts vVnq:c:P:e:D:l:S:H:I: opt
do
	case $opt in
		V) echo $Id: vmoduser_handle.in,v 2.3 2022-08-07 13:08:11+05:30 Cprogrammer Exp mbhangui $;;
		v) verbose=1;;
		n) toggle=1;;
		q) quota="$OPTARG";;
		c) gecos="$OPTARG";;
		P) passwd="$OPTARG";;
		e) epasswd="$OPTARG";;
		l) vacation_file="$OPTARG";;
		S) b64salt="$OPTARG";;
		H) HASH="$OPTARG";;
		I) iter="$OPTARG";;
		?) exit 1;;
	esac
done
if [ -n "$passwd" -o -n "$epasswd" ] ; then
	@prefix@/bin/svc -h @servicedir@/inlookup.infifo
fi
shift $((OPTIND-1))
# now do something with $@
if [ $verbose -ne 0 ] ; then
	@prefix@/bin/vuserinfo $@
fi
