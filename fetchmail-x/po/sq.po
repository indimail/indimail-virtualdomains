msgid "      --limitflush  delete oversized messages\n"
msgstr "      --limitflush  fshi mesazhe të stërmëdhenj\n"

#: options.c:702
msgid "  -n, --norewrite   don't rewrite header addresses\n"
msgstr "  -n, --norewrite   mos rishkruaj adresa kryesh\n"

#: options.c:703
